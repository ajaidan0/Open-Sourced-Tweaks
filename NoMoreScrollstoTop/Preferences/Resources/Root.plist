<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>items</key>
	<array>

		<!-- general enable/ disable switch -->
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>default</key>
			<true/>
			<key>cellClass</key>
  			<string>STTSwitchCell</string>
  			<key>switchColor</key>
  			<string>#BD8AFA</string>
 			 <key>switchColorAlpha</key>
  			<real>1.0</real>
			<key>defaults</key>
			<string>xyz.monotrix.scrollsprefs</string>
			<key>key</key>
			<string>isEnabled</string>
			<key>label</key>
			<string>Enabled</string>
			<key>PostNotification</key>
			<string>xyz.monotrix.scrollsprefs/ReloadPrefs</string>
		</dict>

		<dict>
			<key>cell</key>
			<string>PSLinkCell</string>
			<key>key</key>
			<string>apps</string>
			<key>action</key>
			<string>selectApps</string>
			<key>label</key>
			<string>Select Apps</string>
		</dict>

		<!-- footer -->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>About</string>
		</dict>

		<dict>
			<key>cell</key>
			<string>PSButtonCell</string>
			<key>label</key>
			<string>Source Code</string>
			<key>icon</key>
			<string>github.png</string>
			<key>action</key>
			<string>sourceLink</string>
		</dict>

		<dict>
			<key>cell</key>
			<string>PSButtonCell</string>
			<key>label</key>
			<string>Discord Support Server</string>
			<key>icon</key>
			<string>discord.png</string>
			<key>action</key>
			<string>discordLink</string>
		</dict>

	</array>
	<key>title</key>
	<string>NoMoreScrollstoTop</string>
</dict>
</plist>